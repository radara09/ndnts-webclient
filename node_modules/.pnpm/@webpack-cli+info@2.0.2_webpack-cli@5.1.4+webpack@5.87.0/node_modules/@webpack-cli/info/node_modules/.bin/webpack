#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ndn/web/node_modules/.pnpm/webpack@5.87.0_webpack-cli@5.1.4/node_modules/webpack/bin/node_modules:/home/ndn/web/node_modules/.pnpm/webpack@5.87.0_webpack-cli@5.1.4/node_modules/webpack/node_modules:/home/ndn/web/node_modules/.pnpm/webpack@5.87.0_webpack-cli@5.1.4/node_modules:/home/ndn/web/node_modules/.pnpm/node_modules:/home/ndn/web/node_modules:/home/ndn/node_modules:/home/node_modules:/node_modules:/home/ndn/web/node_modules/.pnpm/@webpack-cli+info@2.0.2_webpack-cli@5.1.4+webpack@5.87.0/node_modules/@webpack-cli/info/node_modules:/home/ndn/web/node_modules/.pnpm/@webpack-cli+info@2.0.2_webpack-cli@5.1.4+webpack@5.87.0/node_modules/@webpack-cli/node_modules:/home/ndn/web/node_modules/.pnpm/@webpack-cli+info@2.0.2_webpack-cli@5.1.4+webpack@5.87.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ndn/web/node_modules/.pnpm/webpack@5.87.0_webpack-cli@5.1.4/node_modules/webpack/bin/node_modules:/home/ndn/web/node_modules/.pnpm/webpack@5.87.0_webpack-cli@5.1.4/node_modules/webpack/node_modules:/home/ndn/web/node_modules/.pnpm/webpack@5.87.0_webpack-cli@5.1.4/node_modules:/home/ndn/web/node_modules/.pnpm/node_modules:/home/ndn/web/node_modules:/home/ndn/node_modules:/home/node_modules:/node_modules:/home/ndn/web/node_modules/.pnpm/@webpack-cli+info@2.0.2_webpack-cli@5.1.4+webpack@5.87.0/node_modules/@webpack-cli/info/node_modules:/home/ndn/web/node_modules/.pnpm/@webpack-cli+info@2.0.2_webpack-cli@5.1.4+webpack@5.87.0/node_modules/@webpack-cli/node_modules:/home/ndn/web/node_modules/.pnpm/@webpack-cli+info@2.0.2_webpack-cli@5.1.4+webpack@5.87.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../webpack@5.87.0_webpack-cli@5.1.4/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../../webpack@5.87.0_webpack-cli@5.1.4/node_modules/webpack/bin/webpack.js" "$@"
fi
