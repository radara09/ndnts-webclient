#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ndn/web/node_modules/.pnpm/terser@5.18.0/node_modules/terser/bin/node_modules:/home/ndn/web/node_modules/.pnpm/terser@5.18.0/node_modules/terser/node_modules:/home/ndn/web/node_modules/.pnpm/terser@5.18.0/node_modules:/home/ndn/web/node_modules/.pnpm/node_modules:/home/ndn/web/node_modules:/home/ndn/node_modules:/home/node_modules:/node_modules:/home/ndn/web/node_modules/.pnpm/node_modules/terser/bin/node_modules:/home/ndn/web/node_modules/.pnpm/node_modules/terser/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ndn/web/node_modules/.pnpm/terser@5.18.0/node_modules/terser/bin/node_modules:/home/ndn/web/node_modules/.pnpm/terser@5.18.0/node_modules/terser/node_modules:/home/ndn/web/node_modules/.pnpm/terser@5.18.0/node_modules:/home/ndn/web/node_modules/.pnpm/node_modules:/home/ndn/web/node_modules:/home/ndn/node_modules:/home/node_modules:/node_modules:/home/ndn/web/node_modules/.pnpm/node_modules/terser/bin/node_modules:/home/ndn/web/node_modules/.pnpm/node_modules/terser/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
